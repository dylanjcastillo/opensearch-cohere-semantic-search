    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"../data/news.csv\", index_col=0).dropna(subset=[\"content\", \"title\", \"description\"]).sample(1500)\n",
    "df[\"text\"] = (\n",
    "    df[\"title\"] + \"\\n\" + df[\"description\"] + \"\\n\" + df[\"content\"]\n",
    ")\n",
    "df.to_csv(\"../data/news_sample.csv\", index=False)"
   ]
  }
